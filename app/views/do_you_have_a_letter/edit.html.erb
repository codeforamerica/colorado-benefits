<% content_for :form_card_title, "What do you have?" %>

<% content_for :form_card_body do %>
    <%= fields_for(:form, @form, builder: CfaFormBuilder) do |f| %>
      <%= f.cfa_checkbox_set_with_none(:proof_types,
        ChangeReportNavigator::PROOF_TYPES.map { |proof_type, label|
          { value: proof_type, label: label }
        },
        label_text: "What do you have?",
        none_text: "I don't have either of these",
        value_is_array: true) %>
    <% end %>
<% end %>

<% content_for :progress_indicator_percentage, "60" %>
